(module
  (import "c4wa" "printf" (func $printf (param i32) (param i32)))
  (memory (export "memory") 1)
  (data (i32.const 1024) "Calling makeStudent()\5Cn\00Student name is %s, access = %d\5Cn\00Vasya\00")
  (func $makeStudent (param $name i32) (param $p_res i32) (result i32)
    (local $res i32)
    (i64.store (i32.const 0) (i64.const 1024))
    (call $printf (i32.const 0) (i32.const 1))
    (set_local $res (i32.const 2048))
    (i32.store (i32.add (get_local $res) (i32.const 4)) (get_local $name))
    (i32.store (i32.add (get_local $res) (i32.const 0)) (i32.const 11))
    (i32.store (get_local $p_res) (get_local $res))
    (return (get_local $res)))
  (func $print_student (param $student i32)
    (i64.store (i32.const 8) (i64.const 1048))
    (i64.store (i32.const 16) (i64.extend_i32_s (i32.load (i32.add (get_local $student) (i32.const 4)))))
    (i64.store (i32.const 24) (i64.extend_i32_s (i32.load (i32.add (get_local $student) (i32.const 0)))))
    (call $printf (i32.const 8) (i32.const 3)))
  (func $main (export "main") (result i32)
    (local $p_student i32)
    (local $student i32)
    (local $@temp_i32 i32)
    (set_local $p_student (i32.const 2058))
    (set_local $@temp_i32 (i32.add (call $makeStudent (i32.const 1082) (get_local $p_student)) (i32.const 0)))
    (i32.store (get_local $@temp_i32) (i32.mul (i32.load (get_local $@temp_i32)) (i32.const 2)))
    (set_local $student (i32.load (get_local $p_student)))
    (call $print_student (get_local $student))
    (i32.store (i32.add (get_local $student) (i32.const 0)) (i32.sub (i32.load (i32.add (get_local $student) (i32.const 0))) (i32.const 1)))
    (call $print_student (i32.load (get_local $p_student)))
    (return (i32.const 0))))
